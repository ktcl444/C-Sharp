			<add name="InvalidArgument" code="20001" message="Invalid argument."/>
			<add name="InvalidOperation" code="20002" message="Invalid Operation performed."/>
			<add name="InvalidPointer" code="20003" message="The object is disposed."/>
			<add name="InvalidSchemaName" code="20004" message="An entity with the specified name already exists. Please specify a unique name."/>

			<add name="UnableToLoadType" code="30001" message="Unable to load type."/>

			<add name="QueryBuilderElementNotFound" code="40001" message="A required element was not specified."/>
			<add name="QueryBuilderAttributeNotFound" code="40002" message="A required attribute was not specified."/>
			<add name="QueryBuilderInvalidAlias" code="40003" message="Invalid alias for aggregate operation."/>
			<add name="QueryBuilderUnexpected" code="49999" message="An unexpected error occurred."/>

			<add name="ConfigurationFileNotFound" code="50001" message="Configuration file : {0} not be found."/>
