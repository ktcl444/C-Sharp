			<add name="ControlViewConfigrationError" code="50004" message=""/>
			<add name="ActionViewConfigrationError" code="50005" message=""/>
			<add name="ViewConfigurationError" code="59998" message=""/>
			<add name="ConfigurationUnexpected" code="59999" message="An unexpected configuration error occurred."/>

			<add name="NotPageAuthorization" code="60001" message=""/>
			<add name="PageParamError" code="60011" message="当前页面无权限访问。"/>
			
			<add name="NotSupported" code="70001" message="This action is not supported."/>
			<add name="NotImplemented" code="70002" message="The requested functionality is not yet implemented."/>
			
			<add name="UnExpected" code="90001" message="An unexpected error occurred."/>
		</messageCodes>
	</exceptionMessageCodeConfiguration>
</configuration>