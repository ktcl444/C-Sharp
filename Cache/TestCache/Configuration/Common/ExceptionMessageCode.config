<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="exceptionMessageCodeConfiguration"
		 type="Mysoft.Map.Common.ExceptionHandling.Configuration.ExceptionMessageCodeConfiguration, Mysoft.Map.Common"/>
	</configSections>
	<exceptionMessageCodeConfiguration>
		<messageCodes>
			<add name="ErrorColumnNotFound" code="10001" message="ColumnNotFoundException: The Column [{0}] Not Found ."/>
